{
  "name": "aiabit-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "cloud:build": "turbo --filter=\"./packages/*\" --filter=\"./apps/cloud\" build",
    "cloud:dev": "turbo --filter=\"./apps/cloud\" dev",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push db:generate",
    "db:studio": "pnpm -F db dev",
    "dev": "turbo dev --parallel",
    "docs:build": "turbo --filter=docs build",
    "docs:dev": "turbo --filter=\"./apps/docs\" dev",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "preinstall": "npx only-allow pnpm",
    "lint": "turbo lint && manypkg check",
    "postinstall": "pnpm db:generate",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "type-check": "turbo type-check",
    "www:build": "turbo --filter=\"./packages/*\" --filter=\"./apps/www\" build",
    "www:dev": "turbo --filter=\"./apps/www\" dev"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@manypkg/cli": "^0.21.0",
    "@vitest/coverage-v8": "^0.33.0",
    "aiabit-scripts": "workspace:*",
    "happy-dom": "^10.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "next": "^13.4.13",
    "only-allow": "^1.1.1",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tsx": "^3.12.7",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "packageManager": "pnpm@7.33.6",
  "engines": {
    "node": "^16.10.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@tanstack/react-query",
        "@tanstack/react-query-next-experimental"
      ]
    }
  }
}